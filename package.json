{
    "name": "scenelang-highlighter",
    "displayName": "SceneLang_Highlighter",
    "description": "Highlighter for the SceneLang Demain-Specific language from the Julia package Raytracer.jl",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.57.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "scenelang",
            "aliases": ["SceneLang", "scenelang"],
            "extensions": [".sl"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "scenelang",
            "scopeName": "source.sl",
            "path": "./syntaxes/scenelang.tmLanguage.json"
        }]
    }
}